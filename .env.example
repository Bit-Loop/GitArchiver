# Download Configuration
DOWNLOAD_DIR=./gharchive_data
MAX_CONCURRENT=10
CHUNK_SIZE=8192

# Processing Configuration
BATCH_SIZE=1000

# Logging Configuration
LOG_LEVEL=INFO
